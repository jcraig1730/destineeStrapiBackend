{
  "kind": "collectionType",
  "collectionName": "sales_orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "shipped": {
      "type": "boolean",
      "default": false
    },
    "subtotal": {
      "type": "decimal"
    },
    "tax": {
      "type": "decimal"
    },
    "shipping": {
      "type": "decimal"
    },
    "total": {
      "type": "decimal"
    },
    "items": {
      "type": "json",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "shippingAddress": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "billingAddress": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    },
    "paymentId": {
      "type": "string"
    }
  }
}
